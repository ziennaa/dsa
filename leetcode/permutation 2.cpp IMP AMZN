class Solution {
    void permutatee(vector<int>& ds, vector<int>& nums, vector<vector<int>>& ans, vector<int>& freq){
         if(ds.size()==nums.size()){
            ans.push_back(ds);
            return;
         }
         for(int i=0; i<nums.size(); i++){
            if (freq[i]) continue;
            if(i>0 && nums[i]==nums[i-1] && !freq[i-1]) continue;
            freq[i] = 1;
            ds.push_back(nums[i]);
            permutatee(ds, nums, ans, freq);
            ds.pop_back();
            freq[i] = 0;
         }
    }
public:
    vector<vector<int>> permuteUnique(vector<int>& nums) {
        sort(nums.begin(), nums.end());    
        vector<vector <int>> ans;
        vector<int> ds;
        vector<int> freq(nums.size(), 0);
        permutatee(ds, nums, ans, freq);
        return ans;
    }
};
